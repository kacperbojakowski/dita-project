    <taskbody class="- topic/body task/taskbody " xtrc="taskbody:1;6:15" xtrf="file:/C:/Users/Kiirama/Desktop/kacper-bojakowski/topics/t_training.dita">
        <context class="- topic/section task/context " xtrc="context:1;7:18" xtrf="file:/C:/Users/Kiirama/Desktop/kacper-bojakowski/topics/t_training.dita">
            <p class="- topic/p " xtrc="p:1;8:16" xtrf="file:/C:/Users/Kiirama/Desktop/kacper-bojakowski/topics/t_training.dita">Cats can be trained through positive conditioning. Follow the steps below to teach
                your cat how to follow you to the kitchen.</p>
        </context>
        <steps class="- topic/ol task/steps " xtrc="steps:1;11:16" xtrf="file:/C:/Users/Kiirama/Desktop/kacper-bojakowski/topics/t_training.dita">
            <step class="- topic/li task/step " importance="optional" xtrc="step:1;12:41" xtrf="file:/C:/Users/Kiirama/Desktop/kacper-bojakowski/topics/t_training.dita">
                <cmd class="- topic/ph task/cmd " xtrc="cmd:1;13:22" xtrf="file:/C:/Users/Kiirama/Desktop/kacper-bojakowski/topics/t_training.dita">Give your cat some food.</cmd>
            </step>
            <step class="- topic/li task/step " importance="optional" xtrc="step:2;15:41" xtrf="file:/C:/Users/Kiirama/Desktop/kacper-bojakowski/topics/t_training.dita">
                <cmd class="- topic/ph task/cmd " xtrc="cmd:2;16:22" xtrf="file:/C:/Users/Kiirama/Desktop/kacper-bojakowski/topics/t_training.dita">Snap your fingers while performing step 1.</cmd>
            </step>
            <step class="- topic/li task/step " importance="required" xtrc="step:3;18:41" xtrf="file:/C:/Users/Kiirama/Desktop/kacper-bojakowski/topics/t_training.dita">
